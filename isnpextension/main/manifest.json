{
    "manifest_version": 2,
    "name": "Phishing Checker",
    "version": "1.0",
    "description": "Check if a website is a phishing site.",
    "permissions": ["<all_urls>"], 
    "browser_action": {
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      },
      "default_popup": "popup.html"
    }
  }
  